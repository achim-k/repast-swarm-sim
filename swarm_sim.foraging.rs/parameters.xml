<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="print_params" displayName="Print parameters" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="expl_strategy" displayName="Exploration Strategy" type="java.lang.String" 
						defaultValue="ComplexQTMemoryCommunication" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									values="Random MemoryComplex MemoryCommunication ComplexCommunication ComplexMemoryCommunication QuadTree ComplexQTMemoryCommunication"
						
		/>
				<parameter name="comm_frequency" displayName="Communication Frequency" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="perception_scope" displayName="Perception scope [m]" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="foraging_strategy" displayName="Foraging Strategy" type="java.lang.String" 
						defaultValue="GoalCommunication" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									values="NoCommunication StateCommunication GoalCommunication "
						
		/>
				<parameter name="agent_count" displayName="Number of Agents" type="int" 
						defaultValue="20" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="resource_count" displayName="Number of resources" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="consecutive_move" displayName="Consecutive moves" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="communication_scope" displayName="Communication scope [m]" type="double" 
						defaultValue="15.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="resource_nest_count" displayName="Number of resource nests" type="int" 
						defaultValue="6" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="use_ga" displayName="Use GA" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="space_dimensions" displayName="Space dimensions (W:H)" type="java.lang.String" 
						defaultValue="100:100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									
		/>
		</parameters>


