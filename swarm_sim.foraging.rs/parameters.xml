<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
	<parameter name="expl_strategy" displayName="Exploration Strategy"
		type="java.lang.String" defaultValue="CC"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
		values="R CM MC CC CMC" />
	<parameter name="foraging_strategy" displayName="Foraging Strategy"
		type="java.lang.String" defaultValue="NC" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
		values="NC SC GC PC" />
	<parameter name="agent_count" displayName="Number of Agents"
		type="int" defaultValue="4" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter name="perception_scope" displayName="Perception scope [%]"
		type="double" defaultValue="1.2" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="communication_scope" displayName="Communication scope [%]"
		type="double" defaultValue="15.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="comm_frequency" displayName="Communication Frequency"
		type="int" defaultValue="1" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter name="resource_count" displayName="Number of resources"
		type="int" defaultValue="0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter name="resource_nest_count" displayName="Number of resource nests"
		type="int" defaultValue="6" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="distance_factor" displayName="Distance Factor D"
		type="double" defaultValue="5.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="segment_count" displayName="segment count N"
		type="int" defaultValue="8" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter name="k" displayName="k" type="double"
		defaultValue="5.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="prob_init" displayName="initial segment probability"
		type="double" defaultValue="0.01" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<parameter name="consecutive_move" displayName="Consecutive moves"
		type="int" defaultValue="0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter name="failure_probability" displayName="Failure Probability [0-1]"
		type="double" defaultValue="0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="decay_rate" displayName="Pheromone decay rate [%]"
		type="double" defaultValue="0.5" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<parameter name="highest_prob" displayName="Take highest prob value" type="boolean"
		defaultValue="false" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter" />
	<parameter name="use_ga" displayName="Use GA" type="boolean"
		defaultValue="false" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter" />
	<parameter name="space_dimensions" displayName="Space dimensions (W:H)"
		type="java.lang.String" defaultValue="100:100" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter" />
	
	<parameter name="print_params" displayName="Print parameters"
		type="boolean" defaultValue="true" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter" />
	<parameter name="randomSeed" displayName="Default Random Seed"
		type="int" defaultValue="__NULL__" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
</parameters>


